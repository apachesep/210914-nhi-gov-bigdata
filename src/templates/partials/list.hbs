{{#if headingTitle}}
<h2 class="tw-text-3xl">{{h2Heading}}</h2>
{{/if}}
{{#if tabSwitcher}}
{{>tabSwitcher tab1name='全部' tabNoline=true tab2name='Tab2' tab3=true tab3name='Tab3' content='nwesTabContent' searchBox=true content2='blank' content3='blank'}}
{{else}}
{{#if searchBox}}
{{>searchBox radioBoxBar=true inputClass='min_width-100@s min_width-3/10@m ' placeholder='conceptual' selectRight1=true selectRight1Label='選項' selectRight1Optionl='題名' secondInput=true secondInputClass='min_width-100@s min_width-3/10@m ' SIPlaceholder='請輸入檢索詞' selectRight2=true selectRight2Label='選項' selectRight2Optionl='題名' searchButtonExpand=false searchButtonExpandClass='uk-width-1-1@s uk-width-auto@l' searchButtonName='縮小查詢範圍' bottomOption=true keywordAndButtons=true colorSecondary=false keywordsTitle='檢索詞：' keyword='conceptual'}}
{{/if}}
{{>pagination simple=false checkedNumber=true orderForMobile=true pageFirstAndLast=true pageSelect=true pageText=true pageSum='2' numberInOnePage=true sortDropdown=true selectLabel='排序選單' selectOptionl='請選擇排序欄位'}}
<table class="uk-table uk-table-small uk-table-striped uk-table-responsive">
	<thead>
		<tr>
			<th class="tw-w-14 tw-font-medium">{{#if check}}<label class="tw-w-full tw-inline-block"><input class="checkAll tw-mr-1 uk-checkbox" type="checkbox">全選</label>{{/if}}</th>
			<th class="min_width-1/6@s tw-font-medium">出處/學術領域</th>
			<th class="min_width-1/3@s tw-font-medium">英文辭彙</th>
			<th class="min_width-1/6@s tw-font-medium">中文辭彙</th>
			<th class="tw-font-medium uk-table-shrink">INFO</th>
		</tr>
	</thead>
	<tbody>
		<tr>
			<td class="uk-text-nowrap">{{#if check}}<label class="tw-w-full tw-inline-block"><input class="listCheck tw-mr-1 uk-checkbox" type="checkbox">1</label>{{else}}1{{/if}}</td>
			<td class="tw-space-x-2 m:tw-space-x-0 uk-text-break">
				<span class="tw-border tw-border-gray-400 tw-px-1 tw-inline-block m:tw-hidden">出處/學術領域</span>
				<span>學術名詞</span>
				<a class="color_primary! tw-pl-1 hover:tw-underline" href="">航空太空名詞</a>
			</td>
			<td class="tw-space-x-2 m:tw-space-x-0 uk-text-break">
				<span class="tw-border tw-border-gray-400 tw-px-1 tw-inline-block m:tw-hidden">英文辭彙</span>
				<a class="hover:tw-underline" href=""><span class="color_primary">conceptual </span><span class="color_secondary">design</span></a>
			</td>
			<td class="tw-space-x-2 m:tw-space-x-0 uk-text-break">
				<span class="tw-border tw-border-gray-400 tw-px-1 tw-inline-block m:tw-hidden">中文辭彙</span>
				<a class="hover:tw-underline" href=""><span class="color_primary">概念設計</span></a>
			</td>
			<td class="tw-space-x-2 m:tw-space-x-0 uk-text-break"><i class="fas fa-info-circle" title="文字說明"></i></td>
		</tr>
		<tr>
			<td class="uk-text-nowrap">{{#if check}}<label class="tw-w-full tw-inline-block"><input class="listCheck tw-mr-1 uk-checkbox" type="checkbox">2</label>{{else}}2{{/if}}</td>
			<td class="tw-space-x-2 m:tw-space-x-0 uk-text-break">
				<span class="tw-border tw-border-gray-400 tw-px-1 tw-inline-block m:tw-hidden">出處/學術領域</span>
				<span>學術名詞</span>
				<a class="color_primary! tw-pl-1 hover:tw-underline" href="">航空太空名詞</a>
			</td>
			<td class="tw-space-x-2 m:tw-space-x-0 uk-text-break">
				<span class="tw-border tw-border-gray-400 tw-px-1 tw-inline-block m:tw-hidden">英文辭彙</span>
				<a class="hover:tw-underline" href=""><span class="color_primary">conceptual </span><span class="color_secondary">design</span></a>
			</td>
			<td class="tw-space-x-2 m:tw-space-x-0 uk-text-break">
				<span class="tw-border tw-border-gray-400 tw-px-1 tw-inline-block m:tw-hidden">中文辭彙</span>
				<a class="hover:tw-underline" href=""><span class="color_primary">概念設計</span></a>
			</td>
			<td class="tw-space-x-2 m:tw-space-x-0 uk-text-break"><i class="fas fa-info-circle" title="文字說明"></i></td>
		</tr>
	</tbody>
</table>
{{>pagination simple=false checkedNumber=true orderForMobile=true pageFirstAndLast=true pageSelect=true pageText=true pageSum='2' numberInOnePage=true sortDropdown=true selectLabel='排序選單' selectOptionl='請選擇排序欄位'}}
{{/if}}